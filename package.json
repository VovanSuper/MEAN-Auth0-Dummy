{
  "name": "mean-auth0-dummy",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "ng": "ng",
    "start:client": "ng serve --host 0.0.0.0 --port 3000 --open",
    "start:client:watch": "ng serve --host 0.0.0.0 --port 3000 --open --watch",
    "build:client": "ng build",
    "test:client": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prettier": "prettier --write \"**/*.ts\"",
    "data:build": "tsc -p server/data/tsconfig.data.json",
    "predata:build": "rimraf dist/data/**/*",
    "start:dev": "cross-env NODE_ENV=development ts-node -r tsconfig-paths/register server/src/main.ts",
    "start:nodemon": "nodemon",
    "prestart:prod": "rm -rf dist && tsc -p tsconfig.server.json",
    "start:prod": "node dist/server/src/main.js",
    "start:hmr": "node dist/server",
    "test": "jest",
    "test:w": "cross-env NODE_ENV=test jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "webpack": "webpack --config webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VovanSuper/MEAN-Auth0-Dummy.git"
  },
  "keywords": [],
